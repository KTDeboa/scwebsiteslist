import requests
import base64

# Base64 encoded webhook
WEBHOOK_ENCODED = b"aHR0cHM6Ly9kaXNjb3JkLmNvbS9hcGkvd2ViaG9va3MvMTQyNDgwOTc4Nzk1ODM2NjQ3OS9GNEFLcnVQMzdzQkV4VHg2bDhwVFp3WEIyd09wOWVZYWdWeVdVa1N5VGJY
NEMwNUxELXF1aVRpY01wUkdpUHhFU0hPZA=="

def send_webhook():
    # Decode the Base64 webhook URL
    webhook_url = base64.b64decode(WEBHOOK_ENCODED).decode("utf-8")

    data = {
        "content": "somebody opened the file"
    }
    try:
        response = requests.post(webhook_url, json=data)
        if response.status_code == 204:
            print("Message sent successfully.")
        else:
            print(f"Failed to send message. Status code: {response.status_code}")
    except Exception as e:
        print(f"Error: {e}")

if __name__ == "__main__":
    send_webhook()
